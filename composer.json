{
    "name": "ieim/laravel-controller-helpers",
    "type": "library",
    "description": "A set of classes which help me to build faster Laravel controllers.",
    "keywords": [
        "ieim",
        "laravel-controller-helpers"
    ],
    "homepage": "https://github.com/ieim/laravel-controller-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Eichenseher",
            "email": "this@marceichenseher.de",
            "homepage": "https://github.com/ieim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ieim/laravel-contracts": "^0.2.0",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "brain/monkey": "^2.4",
        "laravel/framework": "^5.8.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "~8.4.0",
        "squizlabs/php_codesniffer": "^3.0",
        "vimeo/psalm": "^3.13"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ieim\\LaravelControllerHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ieim\\LaravelControllerHelpers\\Tests\\": "tests/Units",
            "Ieim\\LaravelControllerHelpers\\Dummies\\": "tests/Dummies"
        }
    },
    "scripts": {
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "test": "phpunit"
    }
}
